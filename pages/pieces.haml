              %img{src:"../images/navbar/cardsnav.jpg"}
          %li
            %a{:href => "faq.html"}
              %img{src:"../images/navbar/faqnav.jpg"}
          %li
            %a{:href => "contact.html"}
              %img{src:"../images/navbar/contactnav.jpg"}
      .board
